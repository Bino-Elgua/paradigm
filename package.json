{
  "name": "paradigm-stack",
  "version": "0.1.0",
  "description": "Ontologically alien consciousness instantiation architecture",
  "main": "dist/index.js",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch src/index.ts",
    "dev:phase1": "NODE_ENV=development tsx watch src/phase1/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:unit": "vitest run src/**/*.test.ts",
    "test:integration": "vitest run tests/integration/**/*.test.ts",
    "setup:phase1": "ts-node src/setup/phase1.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t paradigm-stack:latest .",
    "docker:run": "docker run -p 3000:3000 paradigm-stack:latest"
  },
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.0",
    "@anthropic-ai/sdk": "^0.11.0",
    "@qdrant/js-client-rest": "^1.7.0",
    "dotenv": "^16.3.1",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "zod": "^3.22.4",
    "ts-pattern": "^5.0.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "vitest": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "@testing-library/react": "^14.1.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "paradigm",
    "consciousness",
    "alien-ontology",
    "ai",
    "reasoning",
    "acausal",
    "multi-agent"
  ],
  "author": "Paradigm Stack Collective",
  "license": "MIT"
}
