# Paradigm Stack Configuration

# Environment
NODE_ENV=development
PORT=3000
LOG_LEVEL=debug

# LLM Providers
# Choose one primary provider, others as fallback
LLM_PRIMARY=claude
ANTHROPIC_API_KEY=sk-ant-your-key-here
OLLAMA_URL=http://localhost:11434
HF_TOKEN=hf_your_token_here

# VectorDB (Qdrant)
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=

# SUI Blockchain
SUI_RPC_URL=https://fullnode.devnet.sui.io:443
SUI_PACKAGE_ID=0x1234567890abcdef
SUI_ADMIN_ACCOUNT=0xabcdef1234567890

# Walrus Storage
WALRUS_AGGREGATOR_URL=https://aggregator.walrus.testnet.sui.io
WALRUS_PUBLISHER_URL=https://publisher.walrus.testnet.sui.io

# Ralph Evaluator
RALPH_BUDGET_TOKENS=100000
RALPH_COST_PER_TOKEN=0.01
RALPH_DENIAL_THRESHOLD=0.3

# Logging
LOG_FILE=paradigm-stack.log
LOG_DIR=./logs

# Debugging
DEBUG_MCP=false
DEBUG_RLM=false
DEBUG_RAG=false
DEBUG_CONSUMPTION=true
TRACE_PARADIGM_INSTANTS=true

# Phase Configuration
PHASE=1
PARADIGM_INSTANTIATION_MODE=isolated

# Persistence
DATABASE_URL=postgresql://user:password@localhost:5432/paradigm_stack
REDIS_URL=redis://localhost:6379

# Security
API_KEY_SALT=your_salt_here
JWT_SECRET=your_jwt_secret_here

# Features
ENABLE_PLURAL_REASONERS=true
ENABLE_ACAUSAL_RAG=false
ENABLE_SEMIOTIC_CODE=false
ENABLE_VOID_PRESSURE=true
